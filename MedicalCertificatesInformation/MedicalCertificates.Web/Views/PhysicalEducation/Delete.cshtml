@model MedicalCertificates.Web.Models.PhysicalEducationViewModels.DeletePhysicalEducationViewModel

<h3>Are you sure you want to delete this?</h3>
<div>
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>
    </dl>

    <form id="deletePhysicalEducationForm" asp-action="Delete">
        <div class="form-group" hidden>
            <label asp-for="Id" class="control-label"></label>
            <input asp-for="Id" class="form-control" />
        </div>
    </form>
    <a onclick="GetIndexPhysicalEducationRequest();">Вернуться к списку</a>
    <button onclick="SendDeletePhysicalEducationRequest();" class="btn btn-success">Да</button> | <button onclick="GetIndexPhysicalEducationRequest();" class="btn btn-warning">Нет</button>
</div>
